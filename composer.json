{
    "name": "capstonelogic/laravel-auth",
    "description": "Light-weight role-based permissions for Laravel 5 built in Auth system.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "permissions",
        "roles",
        "auth",
        "acl",
        "security",
        "eloquent"
    ],
    "authors": [
        {
            "name": "Roman Pavlyshyn",
            "email": "pavlyshyn.roma@gmail.com",
            "homepage": "https://github.com/capstonelogic",
            "role": "Developer"
        },
        {
            "name": "Ahsen M.",
            "homepage": "https://github.com/kodeine",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "doctrine/dbal": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "CapstoneLogic\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CapstoneLogic\\Auth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CapstoneLogic\\Auth\\AuthServiceProvider"
            ]
        }
    }
}
